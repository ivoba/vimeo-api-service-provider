{
    "name": "ivoba/vimeo-api-service-provider",
    "description": "A Vimeo Api ServiceProvider for Silex.",
    "keywords": [
        "silex",
        "vimeo"
    ],
    "homepage": "https://github.com/ivoba/vimeo-api-service-provider",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivo Bathke",
            "email": "ivo.bathke@gmail.com"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "silex/silex": "~1.0",
        "vimeo/vimeo-api" : "1.1.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Ivoba\\Silex\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ivoba\\Silex\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
